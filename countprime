import java.util.*;
class countprime
{
public static void main(String args[])
{
	int n,i,j,t,c=0;
	Scanner sc=new Scanner(System.in);
	System.out.println("enter n value:");
	n=sc.nextInt();
	for(i=2;i<=n;i++)
	{
	t=1;
	for(j=2;j<i;j++)
	{
	if(i%j==0)
	{
	t=0;
	break;
	}
	}
	if(t==1)
	c++;
	}	
	System.out.println(c);
}
}
